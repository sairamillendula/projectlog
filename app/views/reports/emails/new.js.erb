$('<div></div>')
		.html('<%= escape_javascript render("form") %>')
		.dialog({
			autoOpen: true,
			title: 'Share this report via email',
			buttons: { 'Send' :  function(){ $(this).find("form").submit(); }, 'Cancel' : function(){ $(this).dialog("close"); } },
			show: "fade",
			hide: "fade",
			width: 420
    });
