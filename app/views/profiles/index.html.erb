<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>City</th>
    <th>Province</th>
    <th>Postal code</th>
    <th>Country</th>
    <th>Phone number</th>
    <th>Localization</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.company %></td>
    <td><%= profile.address1 %></td>
    <td><%= profile.address2 %></td>
    <td><%= profile.city %></td>
    <td><%= profile.province %></td>
    <td><%= profile.postal_code %></td>
    <td><%= profile.country %></td>
    <td><%= profile.phone_number %></td>
    <td><%= profile.localization %></td>
    <td><%= profile.user_id %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile', new_profile_path %>
