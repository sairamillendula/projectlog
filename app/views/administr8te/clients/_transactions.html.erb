<table>
  <tr>
    <th>Date</th>
    <th>Amount</th>
    <th>Code</th>
  </tr>
  
  <% @user.subscription_transactions.order('created_at DESC').each do |transaction| %>
  <tr>
    <td><%= transaction.created_at.strftime("%d-%m-%Y %H:%M:%S") %></td>
    <td><%= transaction.amount %></td>
    <td><%= transaction.code %></td>
  </tr>
  <% end %>
</table>